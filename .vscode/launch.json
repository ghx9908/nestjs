{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Launch Nest Program",
          "skipFiles": [
              "<node_internals>/**"
          ],
          "runtimeExecutable": "ts-node",
          "runtimeArgs": [
              "-r",
              "tsconfig-paths/register"
          ],
          "args": [
              "${workspaceFolder}/src/main.ts"
          ],
          "cwd": "${workspaceFolder}"
      }
  ]
}
